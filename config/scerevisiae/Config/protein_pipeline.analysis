[SubmitProteome]
module=Dummy
input_id_type=PROTEOME

[SubmitChunk]
module=Dummy
input_id_type=CHUNK


[SubmitTranslation]
module=Dummy
input_id_type=TRANSLATIONID

[scanprosite]
db=Prosite_patterns
db_file=/nfs/seqdb/production/interpro/data/members/prosite/20.52/prosite.patterns
parameters=-confirm => /nfs/panda/ensemblgenomes/production/protein_pipelines/databases/confirm.patterns
module=ProteinAnnotation/PrositeProfile
gff_source=Prosite_pattern
gff_feature=domain
input_id_type=CHUNK

[ncoils]
db=coiled_coil
db_file=/nas/seqdb/integr8/production/code/external/src/coils      
program=ncoils
program_file=/nas/seqdb/integr8/production/code/external/bin/ncoils
module=ProteinAnnotation/Coil
gff_source=ncoils
gff_feature=annotation
input_id_type=CHUNK

[tmhmm]
db=transmembrane
program=decodeanhmm
program_file=/nas/seqdb/integr8/production/code/external/TMHMM2.0c/bin/decodeanhmm
parameters=-modelfile => /nas/seqdb/integr8/production/code/external/TMHMM2.0c/lib/TMHMM2.0.model, -optionsfile => /nas/seqdb/integr8/production/code/external/TMHMM2.0c/lib/TMHMM2.0.options 
module=ProteinAnnotation/Tmhmm
gff_source=Tmhmm
gff_feature=annotation
input_id_type=CHUNK

[Signalp]
db=signal_peptide
program=signalp
program_file=/homes/ensgen/signalp-3.0/signalp
module=ProteinAnnotation/Signalp
gff_source=Signalp
gff_feature=annotation
input_id_type=CHUNK

[pfscan]
db=Prosite_profiles
db_file=/nfs/panda/ensemblgenomes/production/protein_pipelines/databases/prosite.profiles
program=pfscan
program_file=/nfs/seqdb/production/interpro/binaries/64_bit_Linux/pfscan
module=ProteinAnnotation/PrositeProfile
gff_source=Profile
gff_feature=Domain
input_id_type=CHUNK


[Prints]
db=Prints
db_file=/nfs/seqdb/production/interpro/data/iprscan/prints.pval
program=FingerPRINTScan
program_file=/nfs/seqdb/production/interpro/binaries/64_bit_Linux/fingerPRINTScan
module=ProteinAnnotation/Prints
gff_source=Prints
gff_feature=domain
input_id_type=CHUNK


[Pfam]
db=Pfam
db_file=/ebi/production/interpro/data/members/pfam/23.0/Pfam_ls;/ebi/production/interpro/data/members/pfam/23.0/Pfam_fs
program=hmmpfam
program_file=/sw/arch/bin/hmmpfam
parameters=--cut_ga
module=ProteinAnnotation/Pfam
gff_source=Pfam
gff_feature=domain
input_id_type=TRANSLATIONID


[Seg]
db=low_complexity
program=seg
program_file=/nas/seqdb/integr8/production/code/external/bin/seg
module=ProteinAnnotation/Seg
gff_source=Seg
gff_feature=annotation
input_id_type=PROTEOME

[Tigrfam]
db=TIGRfam
db_file=/nfs/seqdb/production/interpro/data/iprscan/TIGRFAMs_HMM.LIB
program=hmmpfam
program_file=/sw/arch/bin/hmmpfam
parameters=--cut_ga
module=ProteinAnnotation/Hmmpfam
gff_source=TIGRFAM
gff_feature=domain
input_id_type=TRANSLATIONID

[Superfamily]
db=Superfamily
db_file=/nfs/seqdb/production/interpro/data/iprscan/superfamily.hmm
program=hmmpfam
program_file=/sw/arch/bin/hmmpfam
parameters=-scopmap => /nfs/seqdb/production/interpro/data/iprscan/superfamily.tab, -evalue => 0.02
module=ProteinAnnotation/Superfamily
gff_source=Superfamily
gff_feature=domain
input_id_type=TRANSLATIONID

[Smart]
db=Smart
db_file=/nfs/seqdb/production/interpro/data/iprscan/smart.HMMs
program=hmmpfam
program_file=/sw/arch/bin/hmmpfam
parameters=-E 0.01
module=ProteinAnnotation/Hmmpfam
gff_source=Smart
gff_feature=domain
input_id_type=TRANSLATIONID

[PIRSF]
db=PIRSF
db_file=/nfs/seqdb/production/interpro/data/iprscan/sf_hmm;/nfs/seqdb/production/interpro/data/iprscan/sf_hmm_sub
program=hmmpfam
program_file=/sw/arch/bin/hmmpfam
parameters=-thresholds => /nfs/seqdb/production/interpro/data/iprscan/pirsf.dat, -options => -E 0.01
module=ProteinAnnotation/PIRSF
gff_source=PIRSF
gff_feature=domain
input_id_type= TRANSLATIONID
